{
  "name": "server-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server-dev": "NODE_ENV=development webpack --watch --config ./config/webpack.server.config --hide-modules",
    "server-pro": "NODE_ENV=production webpack --config ./config/webpack.server.config --progress --hide-modules",
    "client-dev": "NODE_ENV=development webpack --watch --config ./config/webpack.client.config --hide-modules",
    "client-pro": "NODE_ENV=production webpack --config ./config/webpack.client.config --progress --hide-modules",
    "build": "npm run server-pro && npm run client-pro"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "css-loader": "^0.28.5",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "morgan": "~1.8.1",
    "mysql": "^2.14.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "serve-favicon": "~2.4.2",
    "style-loader": "^0.18.2",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vuex": "^2.4.0",
    "vuex-router-sync": "^4.2.0",
    "webpack": "^3.5.5",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
